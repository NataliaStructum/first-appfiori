<mvc:View controllerName="project2basic.controller.Inicio" 
    height="100%"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
	xmlns:f="sap.f"
  	xmlns:core="sap.ui.core"
	xmlns:tnt="sap.tnt" >
	<tnt:ToolPage id="toolPage">
		<tnt:header>
			<f:ShellBar id="_IDGenShellBar1" title="App Name"
				secondTitle="Conocimiento Organizacional"
				showMenuButton="true"
				homeIcon=""
				showCopilot="false"
				showNotifications="true"
				menuButtonPressed="onMenuButtonPress"
				showProductSwitcher="false"
				notificationsNumber="2">
				<f:menu>
					<Menu id="_IDGenMenu1">
						<MenuItem id="_IDGenMenuItem1" text="Flight booking" icon="sap-icon://flight" />
						<MenuItem id="_IDGenMenuItem2" text="Car rental" icon="sap-icon://car-rental" />
					</Menu>
				</f:menu>
				<f:profile>
					<Avatar id="_IDGenAvatar1" initials="UI"/>
				</f:profile>
				
			</f:ShellBar>
		</tnt:header>
		<tnt:sideContent>
			<core:Fragment
					fragmentName="project2basic.view.SideNavigation"
					type="XML" />
		</tnt:sideContent>
		<tnt:mainContents>
			<NavContainer id="pageContainer" initialPage="page2">
			
				<pages>
					<ScrollContainer id="root1" horizontal="false" vertical="true" height="100%">
						<core:Fragment
							fragmentName="project2basic.view.HeaderPerfil"
							type="XML" />
							
					</ScrollContainer>

					<ScrollContainer id="root2" horizontal="false" vertical="true" height="100%">
						<Text id="_IDGenText4" text="This is the root page of the second element"/>
					</ScrollContainer>
					
					<ScrollContainer id="root3" horizontal="false" vertical="true" height="100%">
						<Text id="_IDGenText5" text="This is the root page of the 3 element"/>
					</ScrollContainer>

					<ScrollContainer id="root4" horizontal="false" vertical="true" height="100%">
						<Text id="_IDGenText56" text="This is the root page of the 4 element"/>
					</ScrollContainer>

					<ScrollContainer id="root5" horizontal="false" vertical="true" height="100%">
						<Text id="_IDGenText57" text="This is the root page of the 5 element"/>
					</ScrollContainer>					

				</pages>
			</NavContainer>
		</tnt:mainContents>
	</tnt:ToolPage>
</mvc:View>